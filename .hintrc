{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/text-alternatives": "off",
    "disown-opener": "off",
    "axe/name-role-value": "off",
    "compat-api/html": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "-webkit-overflow-scrolling",
          "scrollbar-width",
          "scrollbar-width: thin",
          "scrollbar-color"
        ]
      }
    ],
    "axe/parsing": [
      "default",
      {
        "marquee": "off"
      }
    ],
    "meta-viewport": "off",
    "button-type": "off"
  }
}